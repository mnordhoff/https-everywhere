<!--
	For other Bitly coverage, see Bitly.xml.

	Nonfunctional domains:

		- api.j.mp *

	* Refused

	Problematic domains:

		- j.mp *
		- www.j.mp *

	* Mismatched, CN: bit.ly
-->
<ruleset name="j.mp">
	<target host="j.mp" />
	<target host="www.j.mp" />

	<securecookie host=".+" name=".+" />

	<rule from="^http://(www\.)?j\.mp/"
		to="https://bit.ly/" />
</ruleset>
